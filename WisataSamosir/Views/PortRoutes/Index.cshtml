@{
    ViewData["Title"] = "dashboard";
    Layout = "_Layout";
}
<div class="col-lg-12">
    <div class="col-lg-12">
        <button type="button" class="btn btn-primary mt-2 mb-2 p-2" onclick="ModalAddRoute()">Add Route</button>
    </div>
</div>
<div class="row" id="card_route">
</div>

<div class="modal" tabindex="-1" role="dialog" id="route">
    <div class="modal-dialog modal-lg" role="document">
        <form id="form_route">
            <div class="modal-content">
                <div class="modal-header">
                    <h5 class="modal-title">Modal title</h5>
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                        <span aria-hidden="true">&times;</span>
                    </button>
                </div>
                <div class="modal-body">
                    <div class="row">
                        <div class="col-md-12">
                            <div class="form-group">
                                <label>Pelabuhan Awal</label>
                                <select class="form-control" id="harbor_start">
                                    <option selected disabled>...</option>
                                </select>
                            </div>
                        </div>
                        <div class="col-md-12">
                            <div class="form-group">
                                <label>Pelabuhan Akhir</label>
                                <select class="form-control" id="harbor_end">
                                    <option selected disabled>...</option>
                                    
                                </select>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="modal-footer">
                    <button class="btn btn-primary" type="button" id="SubmitInsert" onclick="SubmitAddHarbor()">Add Harbor</button>
                    <button class="btn btn-primary" type="button" id="SubmitUpdate" onclick="SubmitUpdateHarbor()">Update</button>
                    <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
                </div>
            </div>
        </form>
    </div>


</div>
@section Scripts{
    <script src="~/port_route/index.js" asp-append-version="true"></script>
}